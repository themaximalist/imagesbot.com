{
    "name": "imagesbot",
    "version": "0.0.4",
    "description": "AI Image Explorer",
    "main": "src/index.js",
    "scripts": {
        "start": "npm run server",
        "dev": "npm run watch",
        "cachebust": "echo $(( $(cat public/cachebust.txt) + 1 )) > public/cachebust.txt",
        "server": "node src/index.js",
        "watch": "nf -j Procfile.dev start",
        "build": "npm run build:css",
        "build:css": "npx tailwindcss -i ./src/app.css -o ./public/app.css",
        "watch:server": "nodemon npm run server",
        "watch:css": "npx tailwindcss -i ./src/app.css -o ./public/app.css --watch"
    },
    "keywords": [
        "image",
        "explorer",
        "ai",
        "stablediffusion"
    ],
    "author": "The Maximalist",
    "license": "MIT",
    "dependencies": {
        "@themaximalist/ai.js": "^0.2.7",
        "cookie-parser": "^1.4.6",
        "debug": "^4.3.4",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-htmx": "^0.0.2",
        "pg": "^8.10.0",
        "sequelize": "^6.31.0",
        "sharp": "^0.32.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.22",
        "tailwindcss": "^3.3.2"
    }
}
